{
  "name": "@onyx/sdk",
  "description": "Enzyme is an Ethereum-based protocol for decentralized on-chain asset management.",
  "version": "1.2.21",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enzymefinance/onyx-sdk.git",
    "directory": "packages/sdk"
  },
  "license": "BUSL-1.1",
  "bugs": {
    "url": "https://github.com/enzymefinance/onyx-sdk/issues"
  },
  "homepage": "https://github.com/enzymefinance/onyx-sdk",
  "scripts": {
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2",
    "build-prepare": "build-utils prepare-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps"
  },
  "peerDependencies": {
    "@onyx/abis": "workspace:*",
    "decimal.js": "^10.4.3",
    "viem": "^2.37.5"
  },
  "devDependencies": {
    "@onyx/abis": "workspace:*",
    "@viem/anvil": "^0.0.10",
    "decimal.js": "^10.4.3",
    "viem": "^2.37.5"
  }
}
