{
  "name": "@enzymefinance/onyx-environment",
  "version": "0.1.0",
  "description": "Onyx is an protocol for decentralized on-chain asset management.",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enzymefinance/onyx-environment.git",
    "directory": "packages/environment"
  },
  "license": "BUSL-1.1",
  "bugs": {
    "url": "https://github.com/enzymefinance/onyx-environment/issues"
  },
  "homepage": "https://github.com/enzymefinance/onyx-environment",
  "scripts": {
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2",
    "build-prepare": "build-utils prepare-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "unregister:expired:pendle:assets": "tsx ./scripts/unregister-expired-pendle-assets.ts",
    "add:pendle:assets:info": "tsx ./scripts/add-pendle-assets-info.ts",
    "deploy:pendle:oracles": "tsx ./scripts/deploy-pendle-oracles/index.ts",
    "reupload:pendle:icons": "tsx ./scripts/reupload-pendle-icons.ts"
  },
  "peerDependencies": {
    "viem": "^2.37.5"
  },
  "devDependencies": {
    "viem": "^2.37.5",
    "typescript": "^5.8.3"
  }
}
