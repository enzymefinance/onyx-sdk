{
  "include": [],
  "compilerOptions": {
    "strict": true,
    "moduleDetection": "force",
    "composite": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "isolatedModules": true,
    "sourceMap": true,
    "declarationMap": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": false,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": false,
    "strictNullChecks": true,
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "incremental": true,
    "removeComments": false,
    "paths": {
      "@onyx/environment": ["./packages/environment/src/index.ts"],
      "@onyx/environment/test": ["./packages/environment/test/setup.ts"],
      "@onyx/environment/*": ["./packages/environment/src/*.ts"],
      "@onyx/sdk": ["./packages/sdk/src/index.ts"],
      "@onyx/sdk/test": ["./packages/sdk/test/setup.ts"],
      "@onyx/sdk/*": ["./packages/sdk/src/*.ts"],
      "@onyx/abis": ["./packages/abis/src/index.ts"],
      "@onyx/abis/*": ["./packages/abis/src/abis/*.ts"]
    }
  }
}
